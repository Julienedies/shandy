{
  "name": "shandy",
  "version": "0.1.3",
  "description": "handy toolkit,",
  "main": "./dist/main/main.js",
  "scripts": {
    "dev": "node .build/webpack/dev-runner.js",
    "dev:main": "cross-env NODE_ENV=develoment webpack --config .build/webpack/main.config.js --watch",
    "dev:renderer": "cross-env NODE_ENV=develoment webpack --config .build/webpack/renderer.config.js --watch",
    "pro": "npm run pro:main && npm run pro:renderer && npm run start",
    "pro:main": "cross-env NODE_ENV=production webpack --config .build/webpack/main.config.js",
    "pro:renderer": "cross-env NODE_ENV=production webpack --config .build/webpack/renderer.config.js",
    "web": "nodemon .build/webpack/web-dev-runner.js --watch .build/webpack/web-dev-runner.js .build/webpack/web.config.js",
    "dev:web": "cross-env NODE_ENV=develoment node .build/webpack/web-dev-runner.js",
    "pro:web": "cross-env NODE_ENV=production node .build/webpack/web-dev-runner.js",
    "start": "electron ./dist/electron/main.js",
    "start:web": "node ./dist/web/server.js",
    "build": "node ./.build/build.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Julienedies/shandy.git"
  },
  "author": "julienedies",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Julienedies/shandy/issues"
  },
  "homepage": "https://github.com/Julienedies/shandy#readme",
  "build": {
    "productName": "shandy",
    "appId": "com.julienedies.shandy",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "@julienedies/brick": "^0.8.5",
    "applescript": "^1.0.0",
    "artyom.js": "^1.0.6",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "chardet": "^0.7.0",
    "cheerio-httpcli": "^0.7.3",
    "chokidar": "^3.0.2",
    "csv": "^5.1.1",
    "debug-menu": "^0.6.1",
    "dialogs": "^1.1.20",
    "echarts": "^4.1.0",
    "electron": "^2.0.17",
    "electron-debug": "^2.0.0",
    "events": "^3.0.0",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "froala-editor": "^2.9.3",
    "fs-extra": "^7.0.1",
    "glob": "^7.1.3",
    "got": "^11.8.2",
    "iconv-lite": "^0.4.23",
    "jhandy": "^0.5.37",
    "jquery": "^3.3.1",
    "jschardet": "^2.1.0",
    "keypress": "^0.2.1",
    "lodash": "^4.17.21",
    "messenger": "0.0.9",
    "moment": "^2.24.0",
    "node-schedule": "^1.3.0",
    "pinyin": "^2.8.3",
    "request": "^2.87.0",
    "request-promise": "^4.2.5",
    "robotjs": "^0.5.1",
    "shelljs": "^0.8.2",
    "socket.io": "^2.1.1",
    "sortablejs": "^1.10.0-rc3",
    "superagent": "^6.1.0",
    "tushare": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "cfonts": "^2.4.2",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "del": "^4.0.0",
    "eslint": "^5.11.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^4.0.3",
    "express": "^4.16.3",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "install": "^0.12.2",
    "mini-css-extract-plugin": "^0.5.0",
    "mkcert": "^1.4.0",
    "multispinner": "^0.2.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.1.5",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
